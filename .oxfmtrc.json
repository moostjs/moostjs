{
  "$schema": "./node_modules/oxfmt/configuration_schema.json",
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "singleQuote": true,
  "trailingComma": "all",
  "insertFinalNewline": true,
  "ignorePatterns": [
    "docs",
    "scripts",
    "packages/create-moost/templates",
    "explorations",
    "packages/**/dist",
    "dist",
    "temp",
    "docs/.vitepress/cache"
  ]
}
