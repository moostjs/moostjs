{
  "extends": "../../api-extractor.json",
  "mainEntryPointFilePath": "./dist/packages/event-cli/src/index.d.ts",
  "dtsRollup": {
    "publicTrimmedFilePath": "./dist/index.d.ts"
  },
  "compiler": {
    "overrideTsconfig": {
      "compilerOptions": {
        "paths": {
          "@moostjs/*": ["packages/event-cli/dist/packages/*/src"],
          "moost": ["packages/event-cli/dist/packages/moost/src"],
          "common": ["packages/event-cli/dist/common"]
        }
      },         
      "include": [
        "packages/event-cli/dist/packages",
        "packages/event-cli/dist/common"
      ]
    }
  }
}