{
  "name": "moostjs",
  "version": "0.3.27",
  "description": "Metadata driven Event Framework inspired by nestjs and powered by wooksjs",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "bootstrap": "node ./scripts/bootstrap",
    "build": "node ./scripts/build",
    "release": "node ./scripts/release",
    "test": "jest --runInBand",
    "test:cov": "jest --runInBand --coverage",
    "lint": "eslint --ext .ts packages/**/src/**/**.ts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "docs": "vitepress dev docs",
    "docs-build": "vitepress build docs",
    "docs-serve": "vitepress serve docs",
    "jiti": "node ./scripts/jiti.js",
    "jiti:dev": "node ./scripts/jiti.js --dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moostjs/moostjs.git"
  },
  "gitHooks": {
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "keywords": [
    "http",
    "wooks",
    "moost",
    "wooksjs",
    "moostjs",
    "web",
    "framework",
    "app",
    "api",
    "rest",
    "restful",
    "prostojs"
  ],
  "author": "Artem Maltsev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moostjs/moostjs/issues"
  },
  "homepage": "https://github.com/moostjs/moostjs#readme",
