{
  "name": "moostjs",
  "version": "0.5.21",
  "description": "Metadata driven Event Framework inspired by nestjs and powered by wooksjs",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "docs": "vitepress dev docs --host \"0.0.0.0\"",
    "docs-build": "vitepress build docs",
    "docs-serve": "vitepress serve docs --host \"0.0.0.0\"",
    "lint": "oxlint -c .oxlintrc.json && eslint --ext .ts packages/**/src/**/**.ts",
    "build": "node ./scripts/build.js",
    "test": "vitest run -c ./vitest.config.js",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "version": "pnpm changelog && git add CHANGELOG.md && git commit -m 'chore: update changelog'",
    "release": "pnpm build && pnpm test && node ./scripts/versions && pnpm -r run pub",
    "sync": "node ./scripts/versions"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moostjs/moostjs.git"
  },
  "keywords": [
    "http",
    "wooks",
    "moost",
    "wooksjs",
    "moostjs",
    "web",
    "framework",
    "app",
    "api",
    "rest",
    "restful",
    "prostojs"
  ],
  "author": "Artem Maltsev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moostjs/moostjs/issues"
  },
  "homepage": "https://github.com/moostjs/moostjs#readme",
  "devDependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-trace-base": "^1.25.1",
    "@prostojs/dye": "^0.4.9",
    "@types/node": "^18.11.9",
    "@types/swagger-ui-dist": "^3.30.1",
    "@typescript-eslint/eslint-plugin": "6.3.0",
    "@typescript-eslint/parser": "6.3.0",
    "conventional-changelog": "^5.1.0",
    "conventional-changelog-cli": "^4.1.0",
    "eslint": "8.55.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-array-func": "4.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jest": "27.6.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sonarjs": "0.23.0",
    "eslint-plugin-unicorn": "49.0.0",
    "eslint-plugin-yml": "1.10.0",
    "husky": "^9.1.7",
    "inquirer": "^12.4.1",
    "oxlint": "^0.15.10",
    "prettier": "3.1.1",
    "rolldown": "1.0.0-beta.3",
    "rollup": "^4.34.6",
    "rollup-plugin-dts": "^6.1.1",
    "typescript": "^5.3.3",
    "unplugin-swc": "^1.5.1",
    "vitepress": "^1.5.0",
    "vitest": "^3.0.5",
    "zx": "^8.3.2"
  }
}
