    "@prostojs/router": "^0.2.1",
    "@prostojs/wf": "^0.0.18",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.11",
    "@types/node": "^18.11.9",
    "@types/prompts": "^2.4.4",
    "@types/swagger-ui-dist": "^3.30.1",
    "@typescript-eslint/eslint-plugin": "6.3.0",
    "@typescript-eslint/parser": "6.3.0",
    "@wooksjs/event-cli": "^0.4.30",
    "@wooksjs/event-core": "^0.4.30",
    "@wooksjs/event-http": "^0.4.30",
    "@wooksjs/event-wf": "^0.4.30",
    "@wooksjs/http-body": "^0.4.30",
    "@wooksjs/http-static": "^0.4.30",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-parameter-decorator": "^1.0.16",
    "babel-plugin-typescript-decorators": "^1.0.3",
    "conventional-changelog": "^5.1.0",
    "conventional-changelog-cli": "^4.1.0",
    "enquirer": "^2.3.6",
    "eslint": "8.55.0",
