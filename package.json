{
  "name": "moostjs",
  "version": "0.6.2",
  "description": "Metadata driven Event Framework inspired by nestjs and powered by wooksjs",
  "keywords": [
    "api",
    "app",
    "framework",
    "http",
    "moost",
    "moostjs",
    "prostojs",
    "rest",
    "restful",
    "web",
    "wooks",
    "wooksjs"
  ],
  "homepage": "https://github.com/moostjs/moostjs#readme",
  "bugs": {
    "url": "https://github.com/moostjs/moostjs/issues"
  },
  "license": "MIT",
  "author": "Artem Maltsev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moostjs/moostjs.git"
  },
  "type": "module",
  "main": "index.js",
  "scripts": {
    "docs": "vitepress dev docs",
    "docs-build": "vitepress build docs",
    "docs-serve": "vitepress serve docs --host \"0.0.0.0\"",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "format": "oxfmt",
    "format:check": "oxfmt --check",
    "build": "node ./scripts/build.js",
    "test": "vitest run -c ./vitest.config.js",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "version": "pnpm changelog && git add CHANGELOG.md && git commit -m 'chore: update changelog'",
    "release": "pnpm build && pnpm test && node ./scripts/versions && pnpm -r run pub",
    "sync": "node ./scripts/versions",
    "postinstall": "packages/event-http/node_modules/@wooksjs/event-http/scripts/setup-skills.js --postinstall && packages/event-http/node_modules/@wooksjs/event-core/scripts/setup-skills.js --postinstall && packages/event-wf/node_modules/@wooksjs/event-wf/scripts/setup-skills.js --postinstall && packages/event-cli/node_modules/@wooksjs/event-cli/scripts/setup-skills.js --postinstall"
  },
  "devDependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-trace-base": "^2.5.1",
    "@prostojs/dye": "^0.4.9",
    "@types/node": "^22.19.11",
    "chart.js": "^4.5.1",
    "conventional-changelog": "^5.1.0",
    "conventional-changelog-cli": "^4.1.0",
    "husky": "^9.1.7",
    "inquirer": "^12.11.1",
    "oxfmt": "^0.34.0",
    "oxlint": "^1.49.0",
    "rolldown": "1.0.0-beta.19",
    "rollup": "^4.59.0",
    "rollup-plugin-dts": "^6.3.0",
    "typescript": "^5.9.3",
    "unplugin-swc": "^1.5.9",
    "vitepress": "^1.6.4",
    "vitepress-plugin-llmstxt": "^0.4.2",
    "vitest": "3.2.4",
    "vue-chartjs": "^5.3.3",
    "zx": "^8.8.5"
  },
  "pnpm": {
    "overrides": {
      "minimatch": "^10.2.3"
    }
  }
}
